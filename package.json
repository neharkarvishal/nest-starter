        "bull": "^3.20.0",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "cron": "^1.8.2",
        "deepmerge": "^4.2.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.3",
        "helmet": "^4.2.0",
